{
  "name": "oregontilth-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "@powershell nvm install $(Get-Content .nvmrc) && @powershell nvm use $(Get-Content .nvmrc)",
    "start": "ng serve --host 0.0.0.0 --disable-host-check true --port 8887",
    "start-ssl": "ng serve --host 0.0.0.0 --disable-host-check true --port 8886 --ssl --ssl-cert dev_cert.pfx --ssl-key password#1",
    "build": "ng build",
    "build-dev": "ng build",
    "build-qa": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=qa --configuration production",
    "build-prod": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=production --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/localize": "^16.2.12",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/platform-server": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@ng-bootstrap/ng-bootstrap": "^15.0.0",
    "@swimlane/ngx-charts": "20.0.0",
    "@tinymce/tinymce-angular": "^6.0.0",
    "ag-grid-angular": "^28.0.1",
    "ag-grid-community": "^28.0.1",
    "angular-oauth2-oidc": "^13.0.0",
    "angular-oauth2-oidc-jwks": "^13.0.0",
    "bootstrap": "^5.2.0",
    "color-scheme": "^1.0.1",
    "core-js": "^3.6.5",
    "font-awesome": "^4.7.0",
    "google-palette": "^1.1.0",
    "iso8601-js-period": "^0.2.1",
    "jquery": "^3.4.1",
    "ng-click-outside2": "^10.0.0",
    "ng-multiselect-dropdown": "^1.0.0",
